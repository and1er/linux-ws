# {{ ansible_managed }}

source ~/.bashrc

# User own binaries.
export PATH=$PATH:{{ user_bin_dir }}

# Golang vars https://golang.org/doc/gopath_code
export PATH=$PATH:{{ user_bin_dir }}/go/bin
export GOPATH=$(go env GOPATH)
export PATH=$PATH:$(go env GOPATH)/bin

# GPG key prompt on Git commits.
export GPG_TTY=$(tty)
